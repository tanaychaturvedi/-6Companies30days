 long long int start=0,end=0,count=0,p=1;
        while(end<n){
            p*=a[end];
            while(start<n and p>=k){
                p=p/a[start];
                start++;
            }
            if(p<k)
            count+=end-start+1;
           
            end++;
        }
        return count;
